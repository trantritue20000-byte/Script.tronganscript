local infJumpEnabled = false
Tab1:AddToggle({
	Name = "B·∫≠t/T·∫Øt Infinite Jump",
	Default = false,
	Callback = function(Value)
		infJumpEnabled = Value
	end
})

game:GetService("UserInputService").JumpRequest:Connect(function()
	if infJumpEnabled then
		local player = game.Players.LocalPlayer
		local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
		if humanoid then
			humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
		end
	end
end)

----------------------------------------------------
-- üîç ESP 1.0 (hi·ªán t√™n & outline ƒë·ªè)
----------------------------------------------------
local ESPEnabled = false
local ESPObjects = {}

local function createESP(player)
	if player == game.Players.LocalPlayer then return end
	player.CharacterAdded:Connect(function(character)
		repeat task.wait() until character:FindFirstChild("Head")

		local highlight = Instance.new("Highlight")
		highlight.Name = "ESPHighlight"
		highlight.FillTransparency = 1
		highlight.OutlineColor = Color3.fromRGB(255, 0, 0)
		highlight.Parent = character
		table.insert(ESPObjects, highlight)

		local nameBillboard = Instance.new("BillboardGui")
		nameBillboard.Name = "ESPName"
		nameBillboard.AlwaysOnTop = true
		nameBillboard.Size = UDim2.new(0, 200, 0, 30)
		nameBillboard.StudsOffset = Vector3.new(0, 2, 0)
		nameBillboard.Parent = character:FindFirstChild("Head")

		local label = Instance.new("TextLabel")
		label.Parent = nameBillboard
		label.BackgroundTransparency = 1
		label.Text = player.Name
		label.TextColor3 = Color3.fromRGB(255, 0, 0)
		label.Font = Enum.Font.SourceSansBold
		label.TextSize = 16
		label.Size = UDim2.new(1, 0, 1, 0)
	end)
end

for _, player in pairs(game.Players:GetPlayers()) do
	if player ~= game.Players.LocalPlayer then
		createESP(player)
	end
end

game.Players.PlayerAdded:Connect(function(player)
	createESP(player)
end)

Tab1:AddToggle({
	Name = "ESP 1.0 (hi·ªán t√™n + vi·ªÅn ƒë·ªè)",
	Default = false,
	Callback = function(Value)
		ESPEnabled = Value
		for _, v in pairs(game.Players:GetPlayers()) do
			if v.Character then
				local highlight = v.Character:FindFirstChild("ESPHighlight")
				local billboard = v.Character:FindFirstChild("Head") and v.Character.Head:FindFirstChild("ESPName")
				if highlight then highlight.Enabled = ESPEnabled end
				if billboard then billboard.Enabled = ESPEnabled end
			end
		end
	end
})

