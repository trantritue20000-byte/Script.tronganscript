-- ========================
-- Spawn teleport button
-- ========================
Tab1:AddButton({
    Name = "D·ªãch chuy·ªÉn v·ªÅ Spawn",
    Callback = function()
        local player = LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

        local spawnLocation = workspace:FindFirstChildOfClass("SpawnLocation")
        if spawnLocation then
            humanoidRootPart.CFrame = spawnLocation.CFrame + Vector3.new(0, 5, 0)
            OrionLib:MakeNotification({Name = "Spawn", Content = "ƒê√£ d·ªãch chuy·ªÉn v·ªÅ ƒëi·ªÉm spawn!", Time = 3})
        else
            OrionLib:MakeNotification({Name = "Spawn", Content = "Kh√¥ng t√¨m th·∫•y ƒëi·ªÉm spawn trong map!", Time = 3})
        end
    end
})

-- ========================
-- Fling button (simple)
-- ========================
Tab1:AddButton({
    Name = "Fling",
    Callback = function()
        local player = LocalPlayer
        local character = player.Character
        if not character or not character:FindFirstChild("HumanoidRootPart") then return end

        OrionLib:MakeNotification({Name = "Fling", Content = "ƒê√£ b·∫≠t ch·∫ø ƒë·ªô Fling! Di chuy·ªÉn g·∫ßn ng∆∞·ªùi kh√°c ƒë·ªÉ h·∫•t vƒÉng h·ªç.", Time = 3})

        local hrp = character.HumanoidRootPart
        local flingPart = Instance.new("BodyThrust")
        flingPart.Force = Vector3.new(999999, 999999, 999999)
        flingPart.Location = Vector3.new(0, 0, 0)
        flingPart.Parent = hrp

        task.wait(10)
        pcall(function() flingPart:Destroy() end)

        OrionLib:MakeNotification({Name = "Fling", Content = "ƒê√£ t·∫Øt ch·∫ø ƒë·ªô Fling sau 10 gi√¢y.", Time = 3})
    end
})

-- ========================
-- Speed: textbox + toggle
-- ========================
Tab1:AddTextbox({
    Name = "Nh·∫≠p t·ªëc ƒë·ªô (VD: 30)",
    Default = "16",
    TextDisappear = true,
    Callback = function(value)
        local num = tonumber(value)
        if num and num > 0 then
            customSpeed = num
            OrionLib:MakeNotification({Name = "T·ªëc ƒë·ªô", Content = "ƒê·∫∑t t·ªëc ƒë·ªô th√†nh: " .. tostring(customSpeed), Time = 3})
        else
            OrionLib:MakeNotification({Name = "L·ªói", Content = "Vui l√≤ng nh·∫≠p s·ªë h·ª£p l·ªá!", Time = 3})
        end
    end
})

Tab1:AddButton({
    Name = "B·∫≠t / T·∫Øt T·ªëc ƒë·ªô",
    Callback = function()
        speedEnabled = not speedEnabled
        local player = LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            if speedEnabled then
                humanoid.WalkSpeed = customSpeed
                OrionLib:MakeNotification({Name = "T·ªëc ƒë·ªô", Content = "ƒê√£ b·∫≠t t·ªëc ƒë·ªô: " .. tostring(customSpeed), Time = 3})
            else
                humanoid.WalkSpeed = 16
                OrionLib:MakeNotification({Name = "T·ªëc ƒë·ªô", Content = "ƒê√£ t·∫Øt, quay l·∫°i t·ªëc ƒë·ªô m·∫∑c ƒë·ªãnh (16)", Time = 3})
            end
        end
    end
})

