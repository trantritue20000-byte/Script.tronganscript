UserInputService.JumpRequest:Connect(function()
  if infJumpEnabled then
    local humanoid = getHumanoid(LocalPlayer)
    if humanoid then
      humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
  end
end)

-- ========================
-- ESP 1.0 (hiá»‡n tÃªn & outline Ä‘á», no bg)
-- ========================
local function ensureESPForCharacter(player, character)
    if not character then return end
    pcall(function()
        if character:FindFirstChild("ESPHighlight") then character.ESPHighlight:Destroy() end
        if character:FindFirstChild("Head") and character.Head:FindFirstChild("ESPName") then character.Head.ESPName:Destroy() end
    end)

    local highlight = Instance.new("Highlight")
    highlight.Name = "ESPHighlight"
    highlight.FillTransparency = 1
    highlight.OutlineColor = Color3.fromRGB(255, 0, 0)
    highlight.Enabled = ESPEnabled
    highlight.Parent = character

    local head = character:FindFirstChild("Head")
    if head then
        local bill = Instance.new("BillboardGui")
        bill.Name = "ESPName"
        bill.AlwaysOnTop = true
        bill.Size = UDim2.new(0, 200, 0, 28)
        bill.StudsOffset = Vector3.new(0, 2, 0)
        bill.Parent = head

        local label = Instance.new("TextLabel")
        label.Name = "ESPLabel"
        label.BackgroundTransparency = 1
        label.BorderSizePixel = 0
        label.Size = UDim2.new(1,0,1,0)
        label.Font = Enum.Font.SourceSansBold
        label.TextSize = 16
        label.Text = player.Name
        label.TextColor3 = Color3.fromRGB(255,0,0)
        label.Parent = bill
        bill.Enabled = ESPEnabled
    end
end

local function createESP(player)
    if player == LocalPlayer then return end
    if player.Character then
        ensureESPForCharacter(player, player.Character)
    end
    player.CharacterAdded:Connect(function(char)
        task.wait(0.3)
        ensureESPForCharacter(player, char)
    end)
end

for _, pl in pairs(Players:GetPlayers()) do
    if pl ~= LocalPlayer then
        createESP(pl)
    end
end

Players.PlayerAdded:Connect(function(pl)
    createESP(pl)
end)

Tab1:AddToggle({
  Name = "ESP 1.0 (hiá»‡n tÃªn + viá»n Ä‘á»)",
  Default = false,
  Callback = function(Value)
    ESPEnabled = Value
    for _, pl in pairs(Players:GetPlayers()) do
        if pl ~= LocalPlayer and pl.Character then
            local h = pl.Character:FindFirstChild("ESPHighlight")
            if h then pcall(function() h.Enabled = ESPEnabled end) end
            local head = pl.Character:FindFirstChild("Head")
            if head and head:FindFirstChild("ESPName") then
                pcall(function() head.ESPName.Enabled = ESPEnabled end)
            end
        end
    end
    OrionLib:MakeNotification({Name="ESP", Content = ESPEnabled and "ESP báº­t" or "ESP táº¯t", Time=3})
  end
})

-- ========================
-- TÃ€NG HÃŒNH (local GUI toggle)
-- ========================
Tab1:AddButton({
  Name = "TÃ€NG HÃŒNH",
  Callback = function()
    local player = LocalPlayer
    local NotificationLibrary = loadstring(game:HttpGet("https://raw.githubusercontent.com/lobox920/Notification-Library/Main/Library.lua", true))()
    NotificationLibrary:SendNotification("Info", "only works in r6 shape", 5.3)
    NotificationLibrary:SendNotification("Success", "I'm a cool message", 6)
    task.wait(6.2)

    -- Gui Setup
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = "InvisibleGui"
    ScreenGui.Parent = player:WaitForChild("PlayerGui")

    local MainFrame = Instance.new("Frame", ScreenGui)
    MainFrame.Size = UDim2.new(0, 200, 0, 100)
    MainFrame.Position = UDim2.new(0.5, -100, 0.5, -50)
    MainFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    MainFrame.BorderColor3 = Color3.fromRGB(255, 255, 255)
    MainFrame.Active = true
    MainFrame.Draggable = true

    local ToggleButton = Instance.new("TextButton", MainFrame)
    ToggleButton.Size = UDim2.new(0.8, 0, 0, 40)
    ToggleButton.Position = UDim2.new(0.1, 0, 0.5, -20)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    ToggleButton.BorderColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.Text = "Toggle Invisible"
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.Font = Enum.Font.SourceSans
    ToggleButton.TextSize = 18

    -- Invisible Script state
    local originalFace = nil
    local accessories = {}
    local invisibleOn = false

    local function makeInvisible()
        if not player.Character then return end
        accessories = {}
        -- Save accessories
        for _, accessory in ipairs(player.Character:GetChildren()) do
            if accessory:IsA("Accessory") then
                table.insert(accessories, accessory:Clone())
            end
        end
        -- Save face
        local head = player.Character:FindFirstChild("Head")
        if head and head:FindFirstChild("face") then
            originalFace = head.face:Clone()
            head.face:Destroy()
        end
        -- Save HRP and move temporarily
        local hrp = player.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            local cloned = hrp:Clone()
            local position = hrp.Position
            task.wait(0.1)
            player.Character:MoveTo(position + Vector3.new(0, 1000000, 0))
            task.wait(0.1)
            if hrp and hrp.Parent then hrp:Destroy() end
            cloned.Parent = player.Character
            player.Character:MoveTo(position)
        end
        -- Make parts semi-transparent and remove accessories
        for _, part in ipairs(player.Character:GetChildren()) do
            if part:IsA("BasePart") then
                pcall(function() part.Transparency = 0.5 end)
            end
        end
        for _, child in ipairs(player.Character:GetChildren()) do
            if child:IsA("Accessory") then
                pcall(function() child:Destroy() end)
            end
        end
    end

    local function makeVisible()
        if not player.Character then return end
        for _, part in ipairs(player.Character:GetChildren()) do
            if part:IsA("BasePart") then
                pcall(function() part.Transparency = 0 end)
            end
        end
        for _, accessory in ipairs(accessories) do
            pcall(function() accessory:Clone().Parent = player.Character end)
        end
        accessories = {}
        if originalFace and player.Character:FindFirstChild("Head") then
            pcall(function() originalFace:Clone().Parent = player.Character.Head end)
        end
        originalFace = nil
    end

    ToggleButton.MouseButton1Click:Connect(function()
        if not invisibleOn then
            makeInvisible()
            ToggleButton.Text = "Invisible On"
        else
            makeVisible()
            ToggleButton.Text = "Toggle Invisible"
        end
        invisibleOn = not invisibleOn
    end)
  end
})

-- ========================
-- Reset character & scripts
-- ========================
Tab1:AddButton({
    Name = "ğŸ” Reset NhÃ¢n Váº­t & Script",
    Callback = function()
        local player = LocalPlayer
        if player.Character then
            player.Character:BreakJoints()
        end

        speedEnabled = false
        customSpeed = 16
        noclipEnabled = false
        infJumpEnabled = false
        ESPEnabled = false
        flying = false

        task.defer(function()
            task.wait(2)
            if player.Character then
                local hum = getHumanoid(player)
                if hum then pcall(function() hum.WalkSpeed = 16 end) end
            end
        end)

        OrionLib:MakeNotification({Name = "Reset", Content = "ÄÃ£ reset nhÃ¢n váº­t vÃ  táº¯t toÃ n bá»™ script!", Image = "rbxassetid://4483345998", Time = 4})
    end
})

