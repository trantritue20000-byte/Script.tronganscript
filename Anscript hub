local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/trantritue20000-byte/Script.tronganscript/main/Anscript%20hub.lua"))()
local x = library:CreateWindow("anscript hub")
local b = x:CreateFolder("player")

b:DestroyGui()

-- Speed
b:Box("Speed", "number", function(val)
    S = val
end)

b:Toggle("Speed", function(bool)
    getgenv().SpeedEnabled = bool
    if bool then
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = S
    else
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
    end
end)

-- Jump
b:Box("Jump", "number", function(val)
    J = val
end)

b:Toggle("Jump", function(bool)
    getgenv().JumpEnabled = bool
    if bool then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = J
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    end
end)

-- Reset
b:Toggle("Reset", function(kill)
    local char = game.Players.LocalPlayer.Character
    if kill then
        char.Humanoid.Health = 0
    else
        char.Humanoid.Health = 100
    end
end)
