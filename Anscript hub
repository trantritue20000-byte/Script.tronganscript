--// AN SCRIPT HUB 1.1 - Cleaned & Fixed (All external loads use direct loadstring)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/trantritue20000-byte/Script.tronganscript/refs/heads/main/Script1.lua'), true))()

local Window = OrionLib:MakeWindow({
  Name = "AN SCRIPT HUB 1.1",
  HidePremium = false,
  SaveConfig = true,
  ConfigFolder = "an_script_hub"
})

--// Shared state variables
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")

local speedEnabled = false
local customSpeed = 16
local noclipEnabled = false
local infJumpEnabled = false
local ESPEnabled = false
local flying = false

--// TAB 1: CHá»¨C NÄ‚NG
local Tab1 = Window:MakeTab({
  Name = "CHá»¨C NÄ‚NG",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

-- create Tab2 early (used later)
local Tab2 = Window:MakeTab({
  Name = "SCRIPT",
  Icon = "rbxassetid://4483345998",
  PremiumOnly = false
})

-- Helper: safe get character/humanoid/hrp
local function getCharacter(player)
    return player and player.Character
end
local function getHumanoid(player)
    local c = getCharacter(player)
    return c and c:FindFirstChildOfClass("Humanoid")
end
local function getHRP(player)
    local c = getCharacter(player)
    return c and c:FindFirstChild("HumanoidRootPart")
end

-- ========================
-- Dá»‹ch chuyá»ƒn vá» Spawn
-- ========================
Tab1:AddButton({
    Name = "Dá»‹ch chuyá»ƒn vá» Spawn",
    Callback = function()
        local player = LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        local spawnLocation = workspace:FindFirstChildOfClass("SpawnLocation")
        if spawnLocation then
            humanoidRootPart.CFrame = spawnLocation.CFrame + Vector3.new(0, 5, 0)
            OrionLib:MakeNotification({Name = "Spawn", Content = "ÄÃ£ dá»‹ch chuyá»ƒn vá» Ä‘iá»ƒm spawn!", Time = 3})
        else
            OrionLib:MakeNotification({Name = "Spawn", Content = "KhÃ´ng tÃ¬m tháº¥y Ä‘iá»ƒm spawn trong map!", Time = 3})
        end
    end
})

-- ========================
-- Fling (simple timed)
-- ========================
Tab1:AddButton({
    Name = "Fling",
    Callback = function()
        local player = LocalPlayer
        local hrp = getHRP(player)
        if not hrp then
            OrionLib:MakeNotification({Name="Fling", Content="KhÃ´ng tÃ¬m tháº¥y HumanoidRootPart", Time=3})
            return
        end

        OrionLib:MakeNotification({Name = "Fling", Content = "ÄÃ£ báº­t Fling (10s). Di chuyá»ƒn gáº§n ngÆ°á»i khÃ¡c Ä‘á»ƒ háº¥t vÄƒng.", Time = 3})

        local flingPart = Instance.new("BodyThrust")
        flingPart.Force = Vector3.new(999999, 999999, 999999)
        flingPart.Location = Vector3.new(0,0,0)
        flingPart.Parent = hrp

        task.delay(10, function()
            pcall(function() flingPart:Destroy() end)
            OrionLib:MakeNotification({Name = "Fling", Content = "Fling Ä‘Ã£ táº¯t.", Time = 3})
        end)
    end
})

-- ========================
-- Speed: textbox + toggle
-- ========================
Tab1:AddTextbox({
    Name = "Nháº­p tá»‘c Ä‘á»™ (VD: 30)",
    Default = "16",
    TextDisappear = true,
    Callback = function(value)
        local num = tonumber(value)
        if num and num > 0 then
            customSpeed = num
            OrionLib:MakeNotification({Name = "Tá»‘c Ä‘á»™", Content = "Äáº·t tá»‘c Ä‘á»™ thÃ nh: " .. tostring(customSpeed), Time = 3})
        else
            OrionLib:MakeNotification({Name = "Lá»—i", Content = "Vui lÃ²ng nháº­p sá»‘ há»£p lá»‡!", Time = 3})
        end
    end
})

Tab1:AddButton({
    Name = "Báº­t / Táº¯t Tá»‘c Ä‘á»™",
    Callback = function()
        speedEnabled = not speedEnabled
        local humanoid = getHumanoid(LocalPlayer)
        if humanoid then
            if speedEnabled then
                humanoid.WalkSpeed = customSpeed
                OrionLib:MakeNotification({Name = "Tá»‘c Ä‘á»™", Content = "ÄÃ£ báº­t tá»‘c Ä‘á»™: "..tostring(customSpeed), Time = 3})
            else
                humanoid.WalkSpeed = 16
                OrionLib:MakeNotification({Name = "Tá»‘c Ä‘á»™", Content = "ÄÃ£ táº¯t, quay vá» 16", Time = 3})
            end
        end
    end
})

