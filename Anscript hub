  Default = false,
  Callback = function(Value)
    noclipEnabled = Value
    OrionLib:MakeNotification({Name="Noclip", Content = noclipEnabled and "Noclip báº­t" or "Noclip táº¯t", Time=3})
  end
})

-- single Stepped handler to avoid multiple connections
RunService.Stepped:Connect(function()
    if noclipEnabled and LocalPlayer and LocalPlayer.Character then
        for _, v in pairs(LocalPlayer.Character:GetDescendants()) do
            if v:IsA("BasePart") then
                pcall(function() v.CanCollide = false end)
            end
        end
    end
end)

