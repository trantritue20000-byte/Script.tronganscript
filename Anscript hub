game:GetService("UserInputService").JumpRequest:Connect(function()
  if infJumpEnabled then
    local humanoid = LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if humanoid then
      humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
    end
  end
end)

-- ========================
-- ESP 1.0 (hiá»‡n tÃªn & outline Ä‘á»)
-- ========================
local function ensureESPForCharacter(player, character)
    if not character then return end
    pcall(function()
        if character:FindFirstChild("ESPHighlight") then character.ESPHighlight:Destroy() end
        if character:FindFirstChild("Head") and character.Head:FindFirstChild("ESPName") then character.Head.ESPName:Destroy() end
    end)

    local highlight = Instance.new("Highlight")
    highlight.Name = "ESPHighlight"
    highlight.FillTransparency = 1
    highlight.OutlineColor = Color3.fromRGB(255, 0, 0)
    highlight.Enabled = ESPEnabled
    highlight.Parent = character

    local head = character:FindFirstChild("Head")
    if head then
        local bill = Instance.new("BillboardGui")
        bill.Name = "ESPName"
        bill.AlwaysOnTop = true
        bill.Size = UDim2.new(0, 200, 0, 28)
        bill.StudsOffset = Vector3.new(0, 2, 0)
        bill.Parent = head

        local label = Instance.new("TextLabel")
        label.Name = "ESPLabel"
        label.BackgroundTransparency = 1
        label.BorderSizePixel = 0
        label.Size = UDim2.new(1,0,1,0)
        label.Font = Enum.Font.SourceSansBold
        label.TextSize = 16
        label.Text = player.Name
        label.TextColor3 = Color3.fromRGB(255,0,0)
        label.Parent = bill
        bill.Enabled = ESPEnabled
    end
end

local function createESP(player)
    if player == LocalPlayer then return end
    if player.Character then
        ensureESPForCharacter(player, player.Character)
    end
    player.CharacterAdded:Connect(function(char)
        task.wait(0.3)
        ensureESPForCharacter(player, char)
    end)
end

for _, pl in pairs(Players:GetPlayers()) do
    if pl ~= LocalPlayer then
        createESP(pl)
    end
end

Players.PlayerAdded:Connect(function(pl)
    createESP(pl)
end)

Tab1:AddToggle({
  Name = "ESP 1.0 (hiá»‡n tÃªn + viá»n Ä‘á»)",
  Default = false,
  Callback = function(Value)
    ESPEnabled = Value
    for _, pl in pairs(Players:GetPlayers()) do
        if pl ~= LocalPlayer and pl.Character then
            local h = pl.Character:FindFirstChild("ESPHighlight")
            if h then pcall(function() h.Enabled = ESPEnabled end) end
            local head = pl.Character:FindFirstChild("Head")
            if head and head:FindFirstChild("ESPName") then
                pcall(function() head.ESPName.Enabled = ESPEnabled end)
            end
        end
    end
    OrionLib:MakeNotification({Name="ESP", Content = ESPEnabled and "ESP báº­t" or "ESP táº¯t", Time=3})
  end
})

-- ========================
-- Reset character & scripts
-- ========================
Tab1:AddButton({
    Name = "ğŸ” Reset NhÃ¢n Váº­t & Script",
    Callback = function()
        local player = LocalPlayer
        if player.Character then
            player.Character:BreakJoints()
        end

        speedEnabled = false
        customSpeed = 16
        noclipEnabled = false
        infJumpEnabled = false
        ESPEnabled = false
        flying = false

        task.defer(function()
            task.wait(2)
            if player.Character then
                local hum = player.Character:FindFirstChildOfClass("Humanoid")
                if hum then pcall(function() hum.WalkSpeed = 16 end) end
            end
        end)

        OrionLib:MakeNotification({Name = "Reset", Content = "ÄÃ£ reset nhÃ¢n váº­t vÃ  táº¯t toÃ n bá»™ script!", Image = "rbxassetid://4483345998", Time = 4})
    end
})

